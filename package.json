{
  "name": "waves-blockchain-explorer",
  "version": "2.0.0",
  "private": true,
  "description": "Waves blockchain information tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/wavesplatform/WavesExplorerLite.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "vendor:dev": "webpack --mode development --config=webpack.vendor.config.js",
    "vendor:prod": "webpack --mode production --config=webpack.vendor.config.js",
    "start": "yarn run vendor:dev && webpack-dev-server --hot --open --mode development",
    "app:dev": "webpack --mode development --config=webpack.config.js",
    "app:prod": "webpack --mode production --config=webpack.config.js",
    "build": "yarn run vendor:prod && yarn run app:prod",
    "build:dev": "yarn run vendor:dev && yarn run app:dev"
  },
  "dependencies": {
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "0.21.0",
    "webpack": "4.16.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
